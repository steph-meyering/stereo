# AWS S3 Configuration
# Get these from your AWS IAM console
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-west-1

# S3 Bucket Names
AWS_DEV_BUCKET=stereo-dev
AWS_PROD_BUCKET=stereo-prod
AWS_SEED_BUCKET=stereo-seeds

# Rails Configuration
# Generate with: openssl rand -hex 64
# SECRET_KEY_BASE=your_secret_key_base_here

# Database Configuration (optional, if not using default)
# DATABASE_URL=postgresql://user:password@localhost/stereo_development
